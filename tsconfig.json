{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "mat-icon-import": [
        "dist/mat-icon-import"
      ],
      "mat-icon-import/*": [
        "dist/mat-icon-import/*"
      ],
      "ng-d3plot": [
        "dist/ng-d3plot"
      ],
      "ng-d3plot/*": [
        "dist/ng-d3plot/*"
      ]
    }
  }
}